{
  "name": "bull-herder",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "docker:start": "docker compose up -d",
    "docker:stop": "docker compose down",
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "test:ci": "npm run lint && npm run test:coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"{lib,test}/**/*.ts\" index.ts"
  },
  "peerDependencies": {
    "bull": "^4.10.0"
  },
  "dependencies": {
    "redis-semaphore": "^5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "bull": "^4.10.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ioredis": "^5.2.3",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  }
}
